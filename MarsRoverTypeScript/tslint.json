{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "linterOptions": {
      "exclude": [
        "**/*.snap"
      ]
    },
    "rules": {
      "object-literal-key-quotes": {
        "options": "as-needed"
      },
      "no-console": [false, "log"],
      "no-require-imports": true,
      "interface-name": [true, "never-prefix"]
    },
    "rulesDirectory": []
  }
  